[Resolve]
{% if dns_servers | length > 0 %}
DNS={{ dns_servers | d(False) | join(' ') }}
{% endif %}
{% if dns_servers_fallback | length > 0 %}
FallbackDNS={{ dns_servers_fallback | join(' ') }}
{% endif %}
{% if dns_domains | length > 0 %}
Domains={{ dns_domains | join(' ') }}
{% endif %}
Cache=no-negative
ReadEtcHosts=yes
DNSStubListener=yes
LLMNR=no
#MulticastDNS=no
#DNSSEC=no
#DNSOverTLS=no
